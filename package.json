{
  "name": "fluorine",
  "module": "./src/index.ts",
  "type": "module",
  "private": "true",
  "scripts": {
    "build": "bun build ./src/api/* --outdir ./src/build && cd ./src/client && bun run build",
    "build-api": " bun build ./src/api/* --outdir ./src/build",
    "build-client": "cd ./src/client && bun run build",
    "start-api": "bun run ./src/build/index.js",
    "start-client": "cd ./src/client && bun run start",
    "dev-api": "bun run ./src/api/index.ts",
    "dev-client": "cd ./src/client && bun run dev"
  },
  "workspaces": [
    "./src/client"
  ],
  "devDependencies": {
    "bun-types": "latest"
  },
  "peerDependencies": {
    "typescript": "^5.0.0"
  }
}